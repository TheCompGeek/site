{
	"version": "2.0.0",
	"scripts": {
		"start": "npm-run-all --parallel dev:*",
		"dev:eleventy": "ELEVENTY_ENV=development eleventy --serve --quiet",
		"dev:parcel": "parcel watch _public/*.{scss,ts} --dist-dir dist --no-source-maps",
		"build": "run-s prod:*",
		"prod:clear": "rm -r dist/ || true",
		"prod:eleventy": "ELEVENTY_ENV=production eleventy",
		"prod:parcel": "parcel build _public/*.{scss,ts} --no-source-maps --no-content-hash --public-url ./"
	},
	"dependencies": {
		"@11ty/eleventy": "2.0.0-canary.4",
		"@11ty/eleventy-plugin-syntaxhighlight": "^4.0.0",
		"@gardeners/markdown-it-wikilinks": "^1.4.3",
		"@parcel/config-default": "^2.3.2",
		"@parcel/core": "^2.3.2",
		"@parcel/optimizer-terser": "^2.3.2",
		"markdown-it": "^12.3.2",
		"markdown-it-anchor": "^8.4.1",
		"markdown-it-attribution": "^0.1.4",
		"markdown-it-attrs": "^4.1.3",
		"markdown-it-container": "^3.0.0",
		"markdown-it-table-of-contents": "^0.6.0",
		"netlify-cli": "^9.13.2",
		"npm-run-all": "^4.1.5",
		"nunjucks": "^3.2.3",
		"parcel": "^2.3.2",
		"pnpm": "^6.32.3",
		"postcss": "^8.4.7",
		"postcss-csso": "^6.0.0",
		"postcss-variable-compress": "^1.0.0",
		"sanitize-filename": "^1.6.3",
		"sass": "1.49.9",
		"typescript": "^4.5.5"
	},
	"devDependencies": {
		"@parcel/transformer-inline-string": "2.3.2",
		"@parcel/transformer-sass": "2.3.2"
	}
}
