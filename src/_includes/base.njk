<!doctype html>
<html lang="en">
   <head>
     {% include "partials/meta.njk" %}
   </head>
   <body>
      {% if color and color != config.color %}
      <style>
        :root {
          --0: {{color}}
        }
      </style>
      {% endif %}
      {% if config.analytics and getEnv("ELEVENTY_ENV","production") %}
        {{ config.analytics | safe }}
      {% endif %}
      <div>
         <header>
           {% include "partials/head.njk" %}
         </header>
         <main>
          <article class="h-entry">
            {% block main %}
              {% if title and not notitle %}
                <h1 class="h-title">{{title}}</h1>
              {% endif %}
              <div class="e-content">
                {{content | safe}}
              </div>
            {% endblock %}
          </article>
         </main>
         <footer>
           {% include "partials/foot.njk" %}
         </footer>
      </div>
   </body>
</html>