<!doctype html>
<html lang="en">
   <head>
     {% include "partials/meta.njk" %}
   </head>
   <body>
      {% if color and color != config.color %}
      <style>
        :root {
          --0: {{color}}
        }
      </style>
      {% endif %}
      {% if not config.disableAnalytics %}
      <noscript>
        <img src="https://shy.boehs.org/ingress/8a1b3eb0-01ba-4347-8250-db6782e755c7/pixel.gif">
      </noscript>
      <script defer src="https://shy.boehs.org/ingress/8a1b3eb0-01ba-4347-8250-db6782e755c7/script.js"></script>
      {% endif %}
      <div>
         <header>
           {% include "partials/head.njk" %}
         </header>
         <main>
          <article>
            {% block main %}
              {% if title and not notitle %}
                <h1>{{title}}</h1>
              {% endif %}
              {{content | safe}}
            {% endblock %}
          </article>
         </main>
         <footer>
           {% include "partials/foot.njk" %}
         </footer>
      </div>
   </body>
</html>